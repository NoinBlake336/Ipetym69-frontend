{
    "builds": [
      {
        "src": "./api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/api/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "/$1",
        "methods": ["GET", "OPTIONS"] 
      }
    ]
  }
  